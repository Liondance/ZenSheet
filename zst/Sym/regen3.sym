sys.reset();

array[8,6] => double a;
array[8,6] => lazy double w;

sys.save("./Out/regen.sym");
sys.reset();
verify | symbols() | = 0
sys.load("./Out/regen.sym");
verify | symbols() | = 2

verify ltype(a) = array[,] => double
verify rtype(a) = array[,] => double

verify ltype(w) = array[,] => lazy double
verify rtype(w) = array[,] => double

