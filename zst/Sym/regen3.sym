verify | symbols() | = 0
sys.load("./Out/regen.sym");
verify | symbols() | = 2

verify ltype(a) = array[,] => var
verify rtype(a) = array[,] => var

verify ltype(w) = array[,] => var
verify rtype(w) = array[,] => var

//

sys.reset();

verify "regen3" <> ?
